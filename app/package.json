{
  "name": "application",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "install-start": "npm install && npm start",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && npm install && tsc -p tsconfig.build.json && copyfiles -E src/**/*.graphql dist/ && chmod +x delete.sh && ./delete.sh",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest || true",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "testCalculation": "jest --forceExit ./src/shared/calculation/calculation.service.spec.ts || true"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "6.2.2",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "^6.2.4",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "2.4.8",
    "babel-jest": "^26.0.1",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.19.0",
    "class-validator": "0.9.1",
    "copyfiles": "2.1.1",
    "dotenv": "7.0.0",
    "graphql": "14.2.1",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "4.0.4",
    "hbs": "4.0.5",
    "mailgun-js": "^0.22.0",
    "multer": "1.4.2",
    "nest-schedule": "^0.6.4",
    "nest-winston": "1.0.4",
    "passport": "0.4.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "password-generator": "2.2.0",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "6.3.3",
    "typeorm": "^0.2.29",
    "unirest": "^0.5.1",
    "uuid": "3.3.2",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "6.0.0",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/supertest": "2.0.7",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "supertest": "3.4.1",
    "testcontainers": "^2.8.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
